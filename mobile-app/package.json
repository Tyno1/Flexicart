{
  "name": "mobile-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build:web": "npx expo export --platform web",
    "build:web:prod": "cp .env.prod .env && yarn build:web",
    "build:android": "eas build --profile production --platform android",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:ios": "eas build --profile production --platform ios",
    "build:all": "eas build  --profile production --platform all",
    "deploy:ios": "eas submit --platform ios",
    "deploy:android": "eas submit --platform android"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/messaging": "^20.4.0",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/native": "^6.0.2",
    "axios": "^1.7.2",
    "eas-cli": "^12.4.1",
    "expo": "~51.0.18",
    "expo-dev-client": "~4.0.25",
    "expo-font": "~12.0.7",
    "expo-image-picker": "~15.0.7",
    "expo-jwt": "^1.8.0",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.17",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-image-picker": "^7.1.2",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-star-rating-widget": "^1.8.0",
    "react-native-svg": "^15.4.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "jest-expo": "~51.0.4",
    "jest": "^29.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.6.1",
    "@types/react": "~18.2.79",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
